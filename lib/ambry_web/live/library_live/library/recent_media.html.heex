<div>
  <%= if @player_states != [] do %>
    <div class="">
      <h1 class="text-3xl mb-6 md:text-4xl md:mb-8 lg:text-5xl lg:mb-12 font-bold">
        Continue listening
      </h1>

      <Amc.player_state_tiles
        player_states={@player_states}
        show_load_more={@show_load_more?}
        load_more={{"load-more", @myself}}
        user={@user}
        browser_id={@browser_id}
      />
    </div>
  <% end %>
</div>
